{
  "expo": {
    "name": "Horibol",
    "slug": "horibol",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/horibol_logo.png",
    "scheme": "horibol",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.horibol.shop",
      "buildNumber": "2",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "horibol",
              "com.googleusercontent.apps.108768584053-oukpfvs3p1b36rcvcs6ue9hsau3qqqt6"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFD700",
        "foregroundImage": "./assets/images/horibol_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.horibol.shop"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFD700",
          "dark": {
            "backgroundColor": "#FFD700"
          }
        }
      ],
      "expo-localization",
      "expo-notifications",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosClientId": "108768584053-oukpfvs3p1b36rcvcs6ue9hsau3qqqt6.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.108768584053-oukpfvs3p1b36rcvcs6ue9hsau3qqqt6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "446a56db-a4ff-4fbc-9011-00375316c823"
      }
    }
  }
}