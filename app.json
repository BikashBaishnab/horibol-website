{
  "expo": {
    "name": "Horibol",
    "slug": "horibol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/horibol_logo.png",
    "scheme": "horibol",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.horibol.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFD700",
        "foregroundImage": "./assets/images/horibol_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.horibol.app",
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFD700",
          "dark": {
            "backgroundColor": "#FFD700"
          }
        }
      ],
      "expo-localization",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "446a56db-a4ff-4fbc-9011-00375316c823"
      }
    }
  }
}
